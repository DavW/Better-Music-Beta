{
   "name": "Better Music Beta for Google Music",
   "description": "Mods Google Music Beta to improve the experience: scrobbling, toast notifications, play controls, and more!",
   "version": "1.3.3",
   "update_url": "http://clients2.google.com/service/update2/crx",
   "icons": {
      "16": "img/logo-16x16.png",
      "48": "img/logo-48x48.png",
      "128": "img/logo-128x128.png"
   },
   "permissions": [ 
      "notifications",
      "tabs",
      "http://music.google.com/",
      "https://music.google.com/",
      "http://ws.audioscrobbler.com/"
   ],
   "browser_action": {
      "default_icon": "img/main-nogmbtab.png",
      "default_title": "Better Music Beta",
      "default_popup": "popup.html"
   },
   "background_page": "background.html",
   "content_scripts": [ 
      {
         "css": [ "css/minimal-music.css" ],
         "js": ["js/inject.js", "js/contentscript.js"],
         "matches": ["http://music.google.com/music/listen*", "https://music.google.com/music/listen*"]
      },
      {
         "js": ["js/jquery-1.6.1.min.js"],
         "matches": ["http://music.google.com/music/listen*", "https://music.google.com/music/listen*"],
         "run_at": "document_start"
      }
   ]
}
